{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "name": "interview/client",
  "description": "Feature rich app with doctrine,postgress, grpc, amqp",
  "require": {
    "php": ">=8.2",
    "ext-amqp": "*",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "doctrine/doctrine-bundle": "^2.9",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.15",
    "fakerphp/faker": "^1.23",
    "friendsofsymfony/rest-bundle": "^3.6.0",
    "google/protobuf": "^3.23",
    "grpc/grpc": "^1.52",
    "jms/serializer-bundle": "^5.2",
    "knpuniversity/oauth2-client-bundle": "^2.15",
    "league/climate": "^3.8",
    "league/oauth2-server": "^8.5",
    "league/oauth2-server-bundle": "^0.5.0",
    "nelmio/api-doc-bundle": "^4.11",
    "nelmio/cors-bundle": "^2.3",
    "open-telemetry/sdk": "^0.0.17",
    "php-amqplib/php-amqplib": "^3.6",
    "php-http/httplug": "^2.4",
    "sentry/sdk": "^3.5",
    "sentry/sentry-symfony": "^4.11",
    "symfony/amqp-messenger": "6.3.*",
    "symfony/asset": "6.3.*",
    "symfony/console": "6.3.*",
    "symfony/dotenv": "6.3.*",
    "symfony/event-dispatcher": "6.3.*",
    "symfony/flex": "^2",
    "symfony/form": "6.3.*",
    "symfony/framework-bundle": "6.3.*",
    "symfony/http-client": "6.3.*",
    "symfony/lock": "6.3.*",
    "symfony/mailer": "6.3.*",
    "symfony/messenger": "6.3.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/rate-limiter": "6.3.*",
    "symfony/runtime": "6.3.*",
    "symfony/security-bundle": "6.3.*",
    "symfony/security-csrf": "6.3.*",
    "symfony/serializer": "6.3.*",
    "symfony/templating": "6.3.*",
    "symfony/translation": "6.3.*",
    "symfony/twig-bundle": "6.3.*",
    "symfony/uid": "6.3.*",
    "symfony/ux-live-component": "^2.6",
    "symfony/validator": "6.3.*",
    "symfony/webpack-encore-bundle": "^2.1",
    "symfony/workflow": "6.3.*",
    "symfony/yaml": "6.3.*",
    "symfonycasts/reset-password-bundle": "^1.18",
    "symfonycasts/verify-email-bundle": "^1.14",
    "twig/extra-bundle": "^3.7",
    "twig/intl-extra": "^3.7"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true,
      "phpro/grumphp": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts",
      "@install-codestandards"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "install-codestandards": [
      "PHPCSStandards\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ],
    "baseline": "phpstan --generate-baseline",
    "bench": "phpbench run",
    "check": [
      "@fix",
      "@stan",
      "@test"
    ],
    "docs": "php generate-class-reference.php",
    "fix": [
      "@rector",
      "@php-cs-fixer"
    ],
    "php-cs-fixer": "php-cs-fixer fix",
    "rector": "rector process --dry-run",
    "stan": "phpstan",
    "test": "phpunit --coverage-html reports/coverage"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.3.*"
    },
    "grumphp": {
      "config-default-path": "./grumphp.yml",
      "project-path": "src/",
      "disable-plugin": false
    },
    "phpcodesniffer-search-depth": 5
  },
  "require-dev": {
    "behat/behat": "^3.13",
    "codeception/codeception": "^5.0",
    "codeception/module-symfony": "^3.1",
    "dama/doctrine-test-bundle": "^7.2",
    "dbrekelmans/bdi": "^1.0",
    "doctrine/coding-standard": "^12.0",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "dq5studios/psalm-junit": "^3.0",
    "edgedesign/phpqa": "^1.26",
    "enlightn/security-checker": "^1.10",
    "escapestudios/symfony2-coding-standard": "^3.13",
    "friendsofphp/php-cs-fixer": "^3.4",
    "infection/infection": "^0.27.6",
    "nunomaduro/phpinsights": "^2.8",
    "orklah/psalm-insane-comparison": "^2.2",
    "pdepend/pdepend": "2.15.1",
    "phing/phing": "2.*",
    "php-parallel-lint/php-console-highlighter": "^1.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "php-standard-library/psalm-plugin": "^2.2",
    "phparkitect/phparkitect": "^0.3.25",
    "phpbench/phpbench": "^1.2",
    "phpmd/phpmd": "@stable",
    "phpmetrics/phpmetrics": "^2.8",
    "phpro/grumphp": "^1.16",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-symfony": "^1.3",
    "phpunit/phpunit": "^10.0",
    "povils/phpmnd": "^3.2",
    "psalm/plugin-mockery": "^1.1",
    "psalm/plugin-phpunit": "^0.18.4",
    "psalm/plugin-symfony": "^5.0",
    "qossmic/deptrac-shim": "^1.0",
    "rector/rector": "^0.18.0",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "~8.0",
    "squizlabs/php_codesniffer": "3.*",
    "symfony/browser-kit": "6.0.*",
    "symfony/css-selector": "6.0.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/panther": "^2.1",
    "symfony/phpunit-bridge": "^6.3",
    "symfony/stopwatch": "6.3.*",
    "symfony/web-profiler-bundle": "6.3.*",
    "symplify/coding-standard": "^12.0",
    "symplify/easy-coding-standard": "^12.0",
    "vimeo/psalm": "^5.15",
    "weirdan/doctrine-psalm-plugin": "^2.9"
  }
}

    "symfony/stopwatch": "~6.4.3",
    "symfony/web-profiler-bundle": "~6.4.4",
    "symplify/coding-standard": "^12.1.4",
    "symplify/easy-coding-standard": "^12.1.14",
    "symplify/phpstan-extensions": "^11.4.3",
    "symplify/phpstan-rules": "^12.4.9",
    "tomasvotruba/class-leak": "^0.2.11",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^0.2.5",
    "vimeo/psalm": "^5.23.1",
    "weirdan/doctrine-psalm-plugin": "^2.9"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php83": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "pestphp/pest-plugin": true,
      "php-http/discovery": true,
      "phpro/grumphp": true,
      "phpro/grumphp-shim": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "grumphp": {
      "config-default-path": "grumphp.yml",
      "disable-plugin": false,
      "project-path": "."
    },
    "phpcodesniffer-search-depth": 5,
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "baseline": "phpstan --generate-baseline",
    "bench": "phpbench run",
    "check": [
      "@fix",
      "@stan",
      "@test"
    ],
    "check-cs": "vendor/bin/ecs check --ansi",
    "docs": "php generate-class-reference.php",
    "fix": [
      "@rector",
      "@php-cs-fixer"
    ],
    "fix-cs": "vendor/bin/ecs check --fix --ansi",
    "install-codestandards": [
      "PHPCSStandards\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
      "vendor/bin/phpcs -q --config-set installed_paths vendor/phpcompatibility/php-compatibility/vendor/doctrine/coding-standard/lib/,vendor/slevomat/coding-standard/,vendor/escapestudios/symfony2-coding-standard/"
    ],
    "php-cs-fixer": "php-cs-fixer fix",
    "phpcs": "phpcs --standard=phpcs.xml",
    "rector": "rector process --dry-run",
    "stan": "phpstan",
    "test": "phpunit --coverage-html reports/phpunit"
  }
}

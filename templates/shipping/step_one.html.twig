{% extends 'logged.html.twig' %}

{% block body %}
    <h1>Shipping Address</h1>

    {{ form_start(form) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.addressLine1) }}
    {{ form_row(form.addressLine2) }}
    {{ form_row(form.city) }}
    {{ form_row(form.state) }}
    {{ form_row(form.postalCode) }}
    {{ form_end(form) }}
{% endblock %}